{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "require-jsdoc": "off",
    "linebreak-style": "off",
    "eol-last": "off",
    "indent": "off"
  },
  "extends": ["eslint:recommended", "google"]
}